    gv_realLevelingStats[EventPlayer()][ge_LevelingStatsDefinition_ExtraTowerDamage] = unitLevel * .25;
    gv_realLevelingStats[EventPlayer()][ge_LevelingStatsDefinition_ExtraCreepItemDropChance] = unitLevel * .008;
    gv_realLevelingStats[EventPlayer()][ge_LevelingStatsDefinition_ExtraAbilityDamage] = unitLevel * .16;
    gv_realLevelingStats[EventPlayer()][ge_LevelingStatsDefinition_ExtraCropSellingValue] = unitLevel * .50;


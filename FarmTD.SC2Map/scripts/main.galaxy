//--------------------------------------------------------------------------------------------------
// Trigger: fasterSpeed
//--------------------------------------------------------------------------------------------------
bool gt_fasterSpeed_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    int currentGameSpeed;
    int newGameSpeed;
    string chatResult = "Game speed is already maxed.";;

    // Conditions
    if (testConds) {
        if (!((GameIsOnline() == false))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    currentGameSpeed = GameGetSpeedValue();
    newGameSpeed = (currentGameSpeed + 1);

    if(currentGameSpeed < 4) { 
        GameSetSpeedValue(newGameSpeed); 
        chatResult = "New game speed is " + IntToString(newGameSpeed);
    }
    
    UIDisplayMessage(PlayerGroupSingle(1), c_messageAreaSubtitle, StringToText(chatResult));
    return true;
}

//--------------------------------------------------------------------------------------------------
// Trigger: slowerSpeed
//--------------------------------------------------------------------------------------------------
bool gt_slowerSpeed_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    int currentGameSpeed;
    int newGameSpeed;
    string chatResult = "Game speed is already at minimum.";

    // Conditions
    if (testConds) {
        if (!((GameIsOnline() == false))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    currentGameSpeed = GameGetSpeedValue();
    newGameSpeed = (currentGameSpeed - 1);

    if(currentGameSpeed > 1) { 
        GameSetSpeedValue(newGameSpeed); 
        chatResult = "New game speed is " + IntToString(newGameSpeed);
    }

    UIDisplayMessage(PlayerGroupSingle(1), c_messageAreaSubtitle, StringToText(chatResult));
    return true;
}


bool gt_onPlayerLevelUp_Func (bool testConds, bool runActions) {
    // Variable Declarations
    int unitLevel = UnitLevel(EventUnit());

    // Actions
    if (!runActions) {
        return true;
    }

    if ((unitLevel >= 0) && (unitLevel < 5)) { UnitBehaviorAdd(EventUnit(), "lvl0", EventUnit(), 1); }
    else if ((unitLevel >= 5) && (unitLevel < 10)) { replaceBehavior(EventUnit(),"lvl0", "lvl5"); }
    else if ((unitLevel >= 10) && (unitLevel < 15)) { replaceBehavior(EventUnit(),"lvl5", "lvl10"); }
    else if ((unitLevel >= 15) && (unitLevel < 20)) { replaceBehavior(EventUnit(),"lvl10", "lvl15"); }

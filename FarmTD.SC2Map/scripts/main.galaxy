    else if ((unitLevel >= 35) && (unitLevel < 40)) { replaceBehavior(EventUnit(),"lvl30", "lvl35"); }
    else if ((unitLevel >= 40) && (unitLevel < 45)) { replaceBehavior(EventUnit(),"lvl35", "lvl40"); }
    else if ((unitLevel >= 45) && (unitLevel < 50)) { replaceBehavior(EventUnit(),"lvl40", "lvl45"); }
    else if ((unitLevel >= 50) && (unitLevel < 55)) { replaceBehavior(EventUnit(),"lvl45", "lvl50"); }
    else if ((unitLevel >= 55) && (unitLevel < 60)) { replaceBehavior(EventUnit(),"lvl50", "lvl55"); }
    else if ((unitLevel >= 60) && (unitLevel < 65)) { replaceBehavior(EventUnit(),"lvl55", "lvl60"); }
    else if ((unitLevel >= 65) && (unitLevel < 70)) { replaceBehavior(EventUnit(),"lvl60", "lvl65"); }
    else if ((unitLevel >= 70) && (unitLevel < 75)) { replaceBehavior(EventUnit(),"lvl65", "lvl70"); }
    else if ((unitLevel >= 75) && (unitLevel < 80)) { replaceBehavior(EventUnit(),"lvl70", "lvl75"); }
    else if ((unitLevel >= 80) && (unitLevel < 85)) { replaceBehavior(EventUnit(),"lvl75", "lvl80"); }
    else if ((unitLevel >= 85) && (unitLevel < 90)) { replaceBehavior(EventUnit(),"lvl80", "lvl85"); }
    else if ((unitLevel >= 90) && (unitLevel < 95)) { replaceBehavior(EventUnit(),"lvl85", "lvl90"); }
    else if ((unitLevel >= 95) && (unitLevel < 100)) { replaceBehavior(EventUnit(),"lvl90", "lvl95"); }
    else if ((unitLevel >= 95)) { replaceBehavior(EventUnit(),"lvl95", "lvl100"); }

    gv_realLevelingStats[EventPlayer()][ge_LevelingStatsDefinition_ExtraStartingMinerals] = unitLevel * 10 + 200;
    gv_realLevelingStats[EventPlayer()][ge_LevelingStatsDefinition_HigherTowerTierChance] = unitLevel * .15;
